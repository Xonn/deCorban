<!DOCTYPE html>
<html>
    <head>
      <meta charset="utf-8">
      <meta name="viewport" content="width=device-width, initial-scale=1.0">
      <meta name="text" content="">
      <meta name="author" content="">
      <link rel="shortcut icon" href="style/images/favicon.png">

        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            <link href="{{ asset('css/bootstrap.min.css') }}" rel="stylesheet" type="text/css"/>
            <link href="{{ asset('css/plugins.css') }}" rel="stylesheet" type="text/css"/>
            <link href="{{ asset('revolution/css/settings.css') }}" rel="stylesheet" type="text/css"/>
            <link href="{{ asset('revolution/css/layers.css') }}" rel="stylesheet" type="text/css"/>
            <link href="{{ asset('revolution/css/navigation.css') }}" rel="stylesheet" type="text/css"/>
            <link href="{{ asset('revolution/revolution-addons/filmstrip/css/revolution.addon.filmstrip.css') }}" rel="stylesheet" type="text/css"/>
            <link href="{{ asset('revolution/revolution-addons/typewriter/css/typewriter.css') }}" rel="stylesheet" type="text/css"/>
  
            <link href="{{ asset('style/icons.css') }}" rel="stylesheet" type="text/css"/>
            <link href="{{ asset('css/style.css') }}" rel="stylesheet" type="text/css"/>
            <link href="{{ asset('css/custom.css') }}" rel="stylesheet" type="text/css"/>
            <link href="{{ asset('css/color/blue-green.css') }}" rel="stylesheet" type="text/css"/>
            <link href='https://fonts.googleapis.com/css?family=Muli:300,300i,400,400i,600,600i,700,700i' rel='stylesheet' type='text/css'>
        {% endblock %}
    </head>

    <body data-aos-easing="ease-in-out-sine" data-aos-duration="800" data-aos-delay="0" style="zoom: 1;">
      <div class="content-wrapper">
        {{ include('component/social_network_bar.html.twig') }}
        {{ include('component/navbar.html.twig') }}
        {% block body %}{% endblock %}
        {{ include('component/footer.html.twig') }}
      </div>

      {% block javascripts %}
        <script src="{{ asset('js/jquery.min.js') }}"></script>
        <script src="{{ asset('js/popper.min.js') }}"></script>
        <script src="{{ asset('js/bootstrap.min.js') }}"></script>
        <script src="{{ asset('revolution/js/jquery.themepunch.tools.min.js') }}"></script>
        <script src="{{ asset('revolution/js/jquery.themepunch.revolution.min.js') }}"></script>
        <script src="{{ asset('revolution/revolution-addons/filmstrip/js/revolution.addon.filmstrip.min.js') }}"></script>
        <script src="{{ asset('revolution/revolution-addons/typewriter/js/revolution.addon.typewriter.min.js') }}"></script>
        <script src="{{ asset('js/plugins.js') }}"></script>
        <script src="{{ asset('js/scripts.js') }}"></script>
        <script src="{{ asset('bundles/fosjsrouting/js/router.min.js') }}"></script>
        <script src="{{ path('fos_js_routing_js', { callback: 'fos.Router.setData' }) }}"></script>
        <script src="{{ asset('js/custom.js') }}"></script>
      {% endblock %}
    </body>
</html>
